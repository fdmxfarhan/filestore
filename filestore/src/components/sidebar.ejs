<div class="search-address">
    <input type="text" id="address-search" placeholder="جستجو در آدرس، نام مالک، متراژ ..." onkeyup="search()">
    <button onclick="search()"><i class="fa fa-search"></i></button>
</div>
<div class="title" id="pro-search-btn">
    <i class="fa fa-search"></i>
    جستجوی پیشرفته
</div>
<div class="search-parameters hidden" id="pro-search-view">
    <div class="range-input">
        <div class="title with-input">متراژ: 
            <!-- <span id="metrage-min">0 </span>
            الی 
            <span id="metrage-max">300 </span> -->
            <label for="metrage-min-input">از</label>
            <input type="number" class="filter-num" name="metrage-min-input" id="metrage-min-input" placeholder="حداقل" value="0">
            <label for="metrage-min-input">تا</label>
            <input type="number" class="filter-num" name="metrage-max-input" id="metrage-max-input" placeholder="حداکثر" value="">
        </div>
        <!-- <div class="slider" id="metrage-slider"></div> -->
    </div>
    <div class="range-input">
        <div class="title with-input">قیمت متری/رهن: 
            <!-- <span id="price2-min">1 </span>
            الی 
            <span id="price2-max">50 میلیون</span> -->
            <label for="price2-min-input">از</label>
            <input type="number" class="filter-num" name="price2-min-input" id="price2-min-input" placeholder="حداقل" value="0">
            <label for="price2-min-input">تا</label>
            <input type="number" class="filter-num" name="price2-max-input" id="price2-max-input" placeholder="حداکثر" value="">
            <label for="price2-min-input">میلیون تومان</label>
        </div>
        <!-- <div class="slider" id="price2-slider"></div> -->
    </div>
    <div class="range-input">
        <div class="title with-input">قیمت کل/اجاره: 
            <!-- <span id="price1-min">100 </span>
            الی 
            <span id="price1-max">500 میلیون</span> -->
            <label for="price1-min-input">از</label>
            <input type="number" class="filter-num" name="price1-min-input" id="price1-min-input" placeholder="حداقل" value="0">
            <label for="price1-min-input">تا</label>
            <input type="number" class="filter-num" name="price1-max-input" id="price1-max-input" placeholder="حداکثر" value="">
            <label for="price1-min-input">میلیون تومان</label>
        </div>
        <!-- <div class="slider" id="price1-slider"></div> -->
    </div>
    
    <div class="range-input">
        <div class="title">سن بنا: 
            <span id="age-min">1 </span>
            الی 
            <span id="age-max">30 </span>
        </div>
        <div class="slider" id="age-slider"></div>
    </div>
    <div class="check-input">
        <div class="title">نوع: </div>
        <div class="item active" id="apartment-btn">آپارتمان</div>
        <div class="item active" id="vilage-btn">ویلایی</div>
        <div class="item active" id="old-btn">کلنگی</div>
        <div class="item active" id="business-btn">تجاری</div>
        <div class="item active" id="office-btn">اداری</div>
        <div class="item active" id="office-estate-btn">موقعیت اداری</div>
        <div class="item active" id="land-btn">زمین</div>
        <div class="item active" id="mostaghelat-btn">مستغلات</div>
        <input type="checkbox" class="check hidden" id="apartment" checked>
        <input type="checkbox" class="check hidden" id="vilage" checked>
        <input type="checkbox" class="check hidden" id="old" checked>
        <input type="checkbox" class="check hidden" id="business" checked>
        <input type="checkbox" class="check hidden" id="office" checked>
        <input type="checkbox" class="check hidden" id="estate" checked>
        <input type="checkbox" class="check hidden" id="land" checked>
        <input type="checkbox" class="check hidden" id="mostaghelat" checked>
    </div>
    <div class="check-input">
        <div class="title">وضعیت: </div>
        <div class="item active" id="sell-btn">فروش</div>
        <div class="item active" id="presell-btn">پیش‌فروش</div>
        <div class="item active" id="exchange-btn">معاوضه</div>
        <div class="item active" id="cooperate-btn">مشارکت</div>
        <div class="item active" id="rent-btn">رهن و اجاره</div>
        <div class="item active" id="rent2-btn">رهن کامل</div>
        <input type="checkbox" class="check hidden" id="sell" checked>
        <input type="checkbox" class="check hidden" id="presell" checked>
        <input type="checkbox" class="check hidden" id="exchange" checked>
        <input type="checkbox" class="check hidden" id="cooperate" checked>
        <input type="checkbox" class="check hidden" id="rent" checked>
        <input type="checkbox" class="check hidden" id="rent2" checked>
    </div>
    <div class="filter-btn" id="filter-btn">
        اعمال تغییرات
    </div>
</div>



<script src="js/sidebar.js"></script>