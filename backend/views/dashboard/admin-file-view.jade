doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    meta(http-equiv='X-UA-Compatible' content='IE=edge')
    meta(name='viewport' content='width=device-width, initial-scale=1.0')
    link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css")
    script(src="/js/jquery-3.5.1.min.js")
    title FileStore
    style.
      @font-face{
      font-family: 'iransans';
      src: url('/css/B-NAZANIN.ttf');
      }
      body{
      margin: 0;
      padding: 0;
      border: none;
      outline: none;
      font-family: 'iransans';
      direction: rtl;
      background-color: white;
      }
      header{
        position: relative;
      overflow: hidden;
      width: 100%;
      margin: auto;
      border-bottom: #2b3546 2px solid;
      }
      header .part.right{
      float: right;
      width: 50%;
      text-align: right;
      }
      header .part.left{
      float: left;
      width: 18vw;
      text-align: justify;
      padding-top: 3vw;
      }
      header .part h1.title{
      font-family: 'iransans';
      font-size: 4vw;
      margin-top: 1vw;
      padding-bottom: 0;
      margin-bottom: 0;
      }
      header .part h2.title{
      font-family: 'iransans';
      font-size: 2vw;
      font-weight: normal;
      padding-top: 0;
      margin-top: 0;
      }
      header .part-center{
        position: absolute;
        top: 0;
        right: 45%;
      }
      header .part-center img{
        width: 7.8vw;
        display: block;
        margin: auto;
        margin-top: 0;
      }
      header .part-center p{
        font-weight: bold;
      margin: 0;
      padding: 0;
      font-family: 'iransans';
      font-size: 1.8vw;
      color: #848C98;
      text-align: center;
      }
      header .part p i{
        padding-left: 0.5vw;
        font-size: 1.8vw;
      }
      header .part p{
      margin: 0;
      padding: 0;
        font-weight: bold;
      font-family: 'iransans';
      font-size: 1.7vw;
      color: #848C98;
      }
      .multi-column-form{
      width: 100%;
      margin: 1vw auto;
      overflow: hidden;
      }
      .multi-column-form .col50{
      float: right;
      width: 50%;
      }
      .multi-column-form .col50 .double-text-input,
      .multi-column-form .col50 .text-area-input{
      overflow: hidden;
      margin-bottom: 1vw;
      }
      .multi-column-form .col50 .double-text-input .label{
        font-weight: bold;
        font-size: 2vw;
      float: right;
      width: 17.3%;
      text-align: left;
      margin-left: 2%;
      padding: 0.2vw 0;
      }
      .multi-column-form .col50 .double-text-input .value{
        font-weight: bold;
        font-size: 1.9vw;
      float: right;
      width: 25%;
      border: #324054 1px solid;
      border-radius: 0.5vw;
      padding: 0.2vw 1vw;
      }
      .multi-column-form .col50 .text-area-input .label{
        font-weight: bold;
        font-size: 1.5vw;
      float: right;
      width: 15%;
      text-align: left;
      margin-left: 2%;
      padding: 0.2vw 0;
      }
      .multi-column-form .col50 .text-area-input .value{
        font-weight: bold;
        font-size: 1.8vw;
      float: right;
      width: 76.4%;
      border: #324054 0.1vw solid;
      border-radius: 0.5vw;
      padding: 0.3vw 1vw;
      min-height: 4.2vw;
      }
      .table-form{
      width: 100%;
      margin: 1vw auto;
      }
      .table-form table{
      width: 100%;
      }
      .table-form th{
      font-family: 'iransans';
      font-size: 1.8vw;
      text-align: center;
      }
      .table-form td{
      overflow: visible;
      }
      .table-form td .value{
        font-weight: bold;
      border: #324054 0.1vw solid;
      border-radius: 0.3vw;
      font-size: 1.8vw;
      padding: 0.3vw 0;
      margin-bottom: 0.2vw;
      text-align: center;
      }
      .long-atext-input,
      .two-text-input{
      width: 100%;
      margin: 1vw auto;
      overflow: hidden;
      }
      .long-atext-input .label{
        font-weight: bold;
        font-size: 2vw;
      float: right;
      width: 5%;
      text-align: left;
      margin-left: 2%;
      padding: 0.2vw 0;
      }
      .long-atext-input .value{
        font-weight: bold;
        font-size: 2vw;
      float: right;
      width: 90%;
      border: #324054 0.1vw solid;
      border-radius: 0.5vw;
      padding: 0.2vw 1vw;
      }
      .two-text-input .label{
        font-size: 2vw;
      float: right;
      width: 10%;
      text-align: left;
      margin-left: 1%;
      padding: 0.2vw 0;
        font-weight: bold;
      }
      .two-text-input .value{
        font-size: 1.9vw;
        min-height: 2vw;
        float: right;
        width: 36%;
        border: #324054 0.1vw solid;
        border-radius: 0.5vw;
        padding: 0.2vw 1vw;
        margin-left: 1vw;
        font-weight: bold;
      }
      .two-text-input .value.marg{margin-left: 0;}
      .value.left{text-align: left;}
      .value.center{text-align: center;}
  body
    header
      .part.right
        h1.title فایل استور
        h2.title فایلینگ تخصصی املاک در تهران
      .part.left
        p 
          i.fa.fa-map-marker
          | بلوار کوهک، انتهای نسیم هفتم، مجتمع تجاری چیتگر، واحد 16
        p.bigger 
          i.fa.fa-phone
          | 46127919
      .part-center
        img(src='/img/logo-dark.png')
        p www.fileestore.ir

    .multi-column-form
      .col50
        .double-text-input
          .label نام مالک
          .value.textinput= typeof(file.ownerName) == 'undefined' || file.ownerName == ''? '-' : file.ownerName
          .label تلفن
          .value.textinput= typeof(file.constPhone) == 'undefined' || file.constPhone == ''? '-' : file.constPhone
        .text-area-input
          .label آدرس
          .value.textinput= typeof(file.address) == 'undefined' || file.address == ''? '-' : file.address
      .col50
        .double-text-input
          .label همراه
          .value.textinput.left= typeof(file.phone) == 'undefined' || file.phone == ''? '-' : file.phone
          .label تاریخ
          .value.textinput.left= typeof(file.date) == 'undefined' || file.date == ''? '-' : file.date
        .double-text-input
          .label نوع
          .value.textinput.center= typeof(file.type) == 'undefined' || file.type == ''? '-' : file.type
          .label شماره
          .value.textinput.left= typeof(file.fileNumber) == 'undefined' || file.fileNumber == ''? '-' : file.fileNumber
        .double-text-input
          .label وضعیت
          .value.textinput.center= typeof(file.state) == 'undefined' || file.state == ''? '-' : file.state
          .label سمت
          .value.textinput.left= typeof(file.role) == 'undefined' || file.role == ''? '-' : file.role
