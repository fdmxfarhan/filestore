      .label امکانات
      .value= typeof(file.options) == 'undefined' || file.options == ''? '-' : file.options
    .two-text-input
      .label قیمت متری/رهن
      .value.left= typeof(file.price) == 'undefined' || file.price == ''? '-' : showPrice(file.price)
      .label کل / اجاره
      .value.left.marg= typeof(file.fullPrice) == 'undefined' || file.fullPrice == ''? '-' : showPrice(file.fullPrice)
