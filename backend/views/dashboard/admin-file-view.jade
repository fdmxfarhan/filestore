            td
              .value= typeof(file.heatingAndCoolingSystem) == 'undefined' || file.heatingAndCoolingSystem == ''? '-' : file.heatingAndCoolingSystem
          tr
            td
              .value= typeof(file.meterage2) == 'undefined' || file.meterage2 == ''? '-' : file.meterage2
            td
