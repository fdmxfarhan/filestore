            td
              .value= typeof(file.unit2) == 'undefined' || file.unit2 == ''? '-' : file.unit2
            td
              .value= typeof(file.buildAge2) == 'undefined' || file.buildAge2 == ''? '-' : file.buildAge2
            td
              .value= typeof(file.parking2) == 'undefined' || file.parking2 == ''? '-' : file.parking2
            td
              .value= typeof(file.warehouse2) == 'undefined' || file.warehouse2 == ''? '-' : file.warehouse2
            td
              .value= typeof(file.elevator2) == 'undefined' || file.elevator2 == ''? '-' : file.elevator2
            td
              .value= typeof(file.kitchen2) == 'undefined' || file.kitchen2 == ''? '-' : file.kitchen2
